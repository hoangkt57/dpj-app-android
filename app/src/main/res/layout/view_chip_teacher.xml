<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.chip.Chip xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:clickable="false"
    android:focusable="false"
    android:paddingTop="4dp"
    android:paddingBottom="4dp"
    android:text="Kirito"
    android:textColor="@color/color_primary_variant"
    android:textSize="13sp"
    app:chipBackgroundColor="@color/student_tag_bg"
    app:chipCornerRadius="3dp"
    app:chipEndPadding="4dp"
    app:chipMinHeight="0dp"
    app:chipMinTouchTargetSize="0dp"
    app:chipStartPadding="0dp"
    app:chipStrokeColor="@color/student_tag_stroke"
    app:chipStrokeWidth="1dp"
    app:closeIcon="@drawable/ic_close"
    app:closeIconSize="14sp"
    app:closeIconTint="@color/color_primary_variant"
    app:closeIconVisible="true"
    app:rippleColor="@null">

</com.google.android.material.chip.Chip>